{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 14",
   
    "compilerOptions": {
      "lib": ["es2015", "dom"],
      "module": "CommonJS",
      "target": "es2020",
   
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": false
    }  
